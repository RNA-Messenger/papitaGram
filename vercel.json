{
  "version": 2,
  "name": "papitaGram",
  "builds": [
    { "use": "@vercel/node", "src": "main.js"}
  ],
  "routes": [
    {
      "src": "(.*).js", "dest": "/$1.js"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    },
    {
      "src": "/.*", "dest": "/index.html"
    }
  ]
}